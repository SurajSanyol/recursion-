#include <iostream>
using namespace std;

int fib(int n)
{
  if(n==0||n==1)
  {
  return n;

  }
  int ans = fib(n-1)+fib(n-2);
  return ans;
}

int main() {
  cout << "Hello world!" << endl;
  int n ;
  cout<<"enter the term that u want to print : ";
  cin>>n;
  int ans = fib(n);
  cout<<ans<<" ";
  return 0;
}
